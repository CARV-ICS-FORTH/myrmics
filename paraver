[0x3E/7] Scheduler 62    
[0x3F/0] Worker 441      
[0x3F/1] Worker 442      
[0x3F/2] Worker 443      
[0x3F/3] Worker 444      
[0x3F/4] Worker 445      
[0x3F/5] Worker 446      
[0x3F/6] Worker 447      
[0x3F/7] Scheduler 63    
[0x4B/0] Scheduler 64    
[0x4B/1] Scheduler 65    
[0x4B/2] Scheduler 66    
[0x4B/3] Scheduler 67    
[0x6B/0] Scheduler 68    
[0x6B/1] Scheduler 69    
[0x6B/2] Scheduler 70    
[0x6B/3] Scheduler 71    
EOF
    ;;

  *) 
    echo -e "\nUsage: paraver <core setup> [<event file>]\n"
    echo -e "For <core setup> select one of the following:\n"
    grep CFG_ $0 | grep -v grep | sed 's/)//'
    echo
    exit 1
    ;;
esac


rm -f paraver.{dec,full,srt,raw,srt2,prv}

if [[ $2 ]]
then
  event_file=$2
else
  event_file="paraver.evt"
fi

# ===========================================================================
# Descramble event file per core and decode it from ASCII85
# ===========================================================================

echo Decoding event file...

for (( i = 0; $i < ${#cpu[*]}; i++ ))
do
  enc=`grep "=== Core $i begin ===" $event_file -A100000 | \
       grep "=== Core $i end ===" -B100000 | \
       grep -v Core`
  if [[ ! $enc ]]
  then
    echo "[1;31m======== ERROR: =======[0m"
    echo "[1m Core $i data not found[0m"
    echo "[1;31m========================[0m"
    exit 1
  fi
  
  echo "$enc" | ./misc/decode_ascii85 > paraver.dec

  exec 3<paraver.dec

  while read -u 3 a
  do
    read -u 3 b
    let tstamp=0x$a
    let evt=0x$b
    echo $i $tstamp $evt >> paraver.full
  done

  exec 3>&-

  rm paraver.dec

done


# ===========================================================================
# Read event file, convert it to decimal and sort it w.r.t. timestamps
# ===========================================================================

echo Sorting events...

sort -k2 -n paraver.full > paraver.srt
rm paraver.full


# ===========================================================================
# Parse sorted file and create intermediate raw one
# ===========================================================================

echo Matching sends/recvs and creating raw file...

exec 4<paraver.srt

declare -A states # Associative array to remember state begins
declare -A sends  # Associative array to remember sends

begin_cnt=0
end_cnt_matched=0
end_cnt_unmatched=0
send_cnt=0
recv_cnt_matched=0
recv_cnt_unmatched=0
max_time=0
while read -r -u 4 core tstamp evt
do
  (( typ = $evt >> 30 ))
  if [ $typ -eq 0 ]
  then
    # Type 0: state begin
    (( id = $evt & 0x3FFFFFFF ))
    states[$core"|"$id]=$tstamp
    #echo State begin $tstamp $core $id
    (( begin_cnt++ ))
  elif [ $typ -eq 1 ]
  then
    # Type 1: state end
    (( id = $evt & 0x3FFFFFFF ))
    #echo State end $tstamp $core $id
    if [ ${states[$core"|"$id]} ]
    then
      echo s ${states[$core"|"$id]} $tstamp $core $id >> paraver.raw
      (( end_cnt_matched++ ))
    else
      (( end_cnt_unmatched++ ))
      #echo "[1;31m^^^ Unmatched![0m"
    fi
  elif [ $typ -eq 2 ]
  then
    # Type 2: send
    (( dst = ($evt >> 20) & 0x3FF ))
    (( id = $evt & 0xFFFFF ))
    sends[$core"|"$dst"|"$id]=$tstamp
    (( send_cnt++ ))
    #echo "Send $core -> $dst at $tstamp, id=$id"
  elif [ $typ -eq 3 ]
  then
    # Type 3: recv
    (( src = ($evt >> 20) & 0x3FF ))
    (( id = $evt & 0xFFFFF ))
    #echo "Recv $core <- $src at $tstamp, id=$id"
    if [ ${sends[$src"|"$core"|"$id]} ]
    then
      echo c ${sends[$src"|"$core"|"$id]} $tstamp $src $core >> paraver.raw
      (( recv_cnt_matched++ ))
    else
      (( recv_cnt_unmatched++ ))
      #echo "[1;31m^^^ Unmatched![0m"
    fi
  fi
  if [ $tstamp -gt $max_time ]
  then
    max_time=$tstamp
  fi
done

rm paraver.srt

echo "  State begins: $begin_cnt"
echo "  State ends: $end_cnt_matched matched, $end_cnt_unmatched unmatched"
echo "  Sends: $send_cnt"
echo "  Receives: $recv_cnt_matched matched, $recv_cnt_unmatched unmatched"

if [ $recv_cnt_unmatched -gt 0 ]
then
  echo "[1;31m================= WARNING: ====================[0m"
  echo "[1m There were $recv_cnt_unmatched unmatched receives[0m"
  echo "[1;31m===============================================[0m"
fi
if [ $end_cnt_unmatched -gt 0 ]
then
  echo "[1;31m================= WARNING: ====================[0m"
  echo "[1m There were $end_cnt_unmatched unmatched states[0m"
  echo "[1;31m===============================================[0m"
fi

# ===========================================================================
# Sort raw file
# ===========================================================================

echo Re-sorting...

sort -k2 -n paraver.raw > paraver.srt2
rm paraver.raw

# ===========================================================================
# Create final trace
# ===========================================================================

echo Creating Paraver trace file...

echo "Paraver (01/01/1789 at 12:00):$max_time:$header" > paraver.prv

exec 5<paraver.srt2

while read -r -u 5 typ tstamp f3 f4 f5
do
  if [[ $typ == "s" ]]
  then
    # State
    echo 1:${cpu[$f4]}:$tstamp:$f3:$f5 >> paraver.prv
  elif [[ $typ == "e" ]]
  then
    # Event (unused for the moment)
    echo 2:${cpu[$f3]}:$tstamp:$f4:0 >> paraver.prv
  elif [[ $typ == "c" ]]
  then
    # Communication
    echo 3:${cpu[$f4]}:$tstamp:$tstamp:${cpu[$f5]}:$f3:$f3 >> paraver.prv
  fi
done

rm paraver.srt2

# ===========================================================================
# Run Paraver
# ===========================================================================
export PARAVER_HOME=/home/lyberis/tools/wxparaver64
export PATH=$PARAVER_HOME/bin:$PATH

wxparaver paraver.prv paraver.cfg

