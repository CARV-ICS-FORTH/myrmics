#!/bin/bash

ROW_FILE="paraver.row"

case "$1" in

# ===========================================================================
  CFG_2_HET_FLAT)
# ===========================================================================
    header="2(1,1):1:2(1:1,1:2)"
    cpu[0]="1:1:1:1"
    cpu[1]="2:1:2:1"

    cat > $ROW_FILE << EOF
LEVEL TASK SIZE 2
Formic 0x2B
ARM 0x6B

LEVEL CPU SIZE 2
[0x2B/0] Worker 0
[0x6B/0] Scheduler 0
EOF
    ;;

